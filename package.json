{
  "name": "rapid-firebase-authentication",
  "version": "1.2.0",
  "description": "A React-based module to be used in rapid prototyping to quickly add registration/login/reset functionality to a firebase-backed application",
  "main": "dist/RapidFirebaseLogin.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext '.js,.jsx' src",
    "prepublish": "npm run test && npm run lint && webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developertown/rapid-firebase-authentication.git"
  },
  "keywords": [
    "firebase",
    "react",
    "authentication"
  ],
  "author": {
    "name": "DeveloperTown",
    "url": "https://github.com/developertown"
  },
  "contributors": [
    {
      "name": "Jon Nolen",
      "url": "https://github.com/jonnolen"
    },
    {
      "name": "Nick Kiser",
      "url": "https://github.com/nkiser"
    },
    {
      "name": "Jason Vasquez",
      "url": "https://github.com/jasonvasquez"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/developertown/rapid-firebase-authentication/issues"
  },
  "homepage": "https://github.com/developertown/rapid-firebase-authentication#readme",
  "peerDependencies": {
    "firebase": "^3.0.1",
    "react": "^0.14.7"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-jest": "^9.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "~2.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "firebase": "^3.0.1",
    "jest-cli": "^0.9.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "webpack": "^1.12.14"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "commonjs": true,
      "jest": true,
      "jasmine": true
    },
    "parserOptions": {
      "ecmaVersion": 7
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
